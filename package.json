{
	"name": "observly-api",
	"version": "0.0.0",
	"description": "",
	"main": "src/index.ts",
	"typings": "src/typings.d.ts",
	"author": "Alex Acebo",
	"license": "ISC",
	"scripts": {
		"test": "npm run test:unit && npm run test:int",
		"test:unit": "npx cross-env NODE_ENV=test jest \".spec\\.ts\"",
		"test:int": "npx cross-env NODE_ENV=test jest \".test\\.ts\" --forceExit --runInBand --detectOpenHandles --collectCoverage=false",
		"start": "npx cross-env DEBUG=observly-api* ts-node .",
		"start:local": "npx cross-env NODE_ENV=local DEBUG=observly-api* nodemon --watch 'src/**/*' -e ts --exec ts-node .",
		"lint": "npx eslint . --ext .ts",
		"lint:fix": "npx eslint . --ext .ts --fix",
		"prettier": "npx prettier --config .prettierrc.json --check .",
		"prettier:fix": "npx prettier --config .prettierrc.json --check --write .",
		"migrate:make": "npx cross-env NODE_ENV=local knex --knexfile knexfile.ts migrate:make -x ts",
		"migrate:latest": "npx cross-env NODE_ENV=local knex --knexfile knexfile.ts migrate:latest",
		"migrate:rollback": "npx cross-env NODE_ENV=local knex --knexfile knexfile.ts migrate:rollback",
		"seed:make": "npx cross-env NODE_ENV=local knex --knexfile knexfile.ts seed:make",
		"seed:run": "npx cross-env NODE_ENV=local knex --knexfile knexfile.ts seed:run"
	},
	"keywords": [
		"observe",
		"api",
		"realtime"
	],
	"dependencies": {
		"@koa/router": "^10.0.0",
		"camelcase": "^6.2.0",
		"chalk": "^4.1.1",
		"date-fns": "^2.21.3",
		"debug": "^4.3.1",
		"dotenv": "^9.0.2",
		"faker": "^5.5.3",
		"http-status-codes": "^2.1.4",
		"joi": "^17.4.0",
		"knex": "^0.95.5",
		"koa": "^2.13.1",
		"pg": "^8.6.0",
		"reflect-metadata": "^0.1.13",
		"slugify": "^1.5.3",
		"snake-case": "^3.0.4",
		"tsyringe": "^4.5.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/debug": "^4.1.5",
		"@types/faker": "^5.5.5",
		"@types/jest": "^26.0.23",
		"@types/koa": "^2.13.1",
		"@types/koa__router": "^8.0.4",
		"@types/node": "^15.0.2",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.26.0",
		"eslint-plugin-github": "^4.1.3",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.3.6",
		"jest": "^26.6.3",
		"nodemon": "^2.0.7",
		"prettier": "^2.3.0",
		"ts-jest": "^26.5.6",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.4"
	}
}
